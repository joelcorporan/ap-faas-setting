{
    "extends": "airbnb-base",
    "plugins": [
        "import"
    ],
    "globals": {
      "afterAll": true,
      "afterEach": true,
      "beforeAll": true,
      "beforeEach": true,
      "describe": true,
      "expect": true,
      "fail": true,
      "it": true,
      "jasmine": true,
      "jest": true,
      "test": true
    },
    "rules": {
      "indent": ["error", 2],
      "no-tabs": 0,
      "no-trailing-spaces": 0,
      "no-console": 0,
      "padded-blocks": 0,
      "camelcase": 0,
      "object-shorthand": 0,
      "no-underscore-dangle": 0,
      "prefer-destructuring": 0,
      "spaced-comment": 0,
      "consistent-return": 0,
      "import/no-extraneous-dependencies": 0,
      "no-unused-vars": 0,
      "no-shadow": 0,
      "default-param-last": 0,
      "no-multi-str": 0,
      "class-methods-use-this": 0
    }
}
